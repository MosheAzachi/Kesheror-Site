<% extend('base') %>
<div class="block">
    <h1>סל קניות</h1>
    <div class="catalog">
        <% items.forEach(function(item) { %>
            <div class="item">
                <img src="<%= item.image %>" alt="<%= item.productName %>">
                <h2><%= item.productName%></h2>
                <p><%= item.description %></p>
                <b><%= item.price %> ש"ח</b>
                <div class="center">
                    <button type="button" class="btn btn-primary btn-block mb-4 itemBtn">מחק</button>
                </div>
            </div>
        <% }); %>
    </div>
    <hr>
</div>